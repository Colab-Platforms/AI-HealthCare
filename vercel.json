{
  "version": 2,
  "buildCommand": "cd client && rm -rf dist && npm install && npm run build",
  "outputDirectory": "client/dist",
  "builds": [
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/env-check.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/ai-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/env-check",
      "dest": "/api/env-check.js"
    },
    {
      "src": "/api/ai-test",
      "dest": "/api/ai-test.js"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
}
